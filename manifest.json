{

  "description": "Like Cenafy, but for Firefox.",
  "manifest_version": 2,
  "name": "UnexpectedCena",
  "version": "0.1",
  "icons": {
    "48": "icons/JC.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*/"],
      "js": ["jquery-3.3.1.min.js", "rollCena.js"]
    }
  ],
  
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ]

}
